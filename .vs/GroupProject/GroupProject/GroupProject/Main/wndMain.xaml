<Window x:Class="GroupProject.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GroupProject"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Menu x:Name="Menu" Height="48" VerticalAlignment="Top" Margin="0,0,386,0">
            <MenuItem x:Name="itemSearch" Header="Search" Click="itemSearch_Click"></MenuItem>
            <MenuItem x:Name="itemItems" Header="Items" Click="itemItems_Click"/>
        </Menu>
    </Grid>
</Window>
